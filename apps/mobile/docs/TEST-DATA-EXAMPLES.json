  "test_scenarios": {
    "scenario_1_complete_flow": {
      "name": "Flusso Completo: Crea Prodotto → Crea Manutenzione",
      "steps": [
        "1. POST /product/create con dati 'Segnale Pericolo - Milano'",
        "2. Salvare uuid dalla risposta",
        "3. POST /maintenance/create usando uuid del prodotto",
        "4. Verificare transaction_id nella risposta",
        "5. GET /product per vedere il prodotto nell'elenco",
        "6. GET /maintenance per vedere la manutenzione nell'elenco"
      ]
    },
    "scenario_2_duplicate_test": {
      "name": "Test Duplicato: Stesso QR Code due volte",
      "steps": [
        "1. POST /product/create con qr_code='DUPLICATE-TEST'",
        "2. Verificare success (200)",
        "3. POST /product/create con stesso qr_code='DUPLICATE-TEST'",
        "4. Verificare errore 400 'SHA-256 already exists'"
      ]
    },
    "scenario_3_gps_validation": {
      "name": "Test GPS Validation",
      "steps": [
        "1. POST /maintenance/create con GPS valido (45.464211, 9.189982)",
        "2. Verificare success (200)",
        "3. POST /maintenance/create con GPS invalido (1234.567890, 9.189982)",
        "4. Verificare errore 400 GPS validation"
      ]
    }
  },

  "quick_copy_paste": {
    "description": "Comandi pronti per copy/paste nel terminale",
    "setup": [
      "export API_URL='http://localhost:3000'",
      "export JWT_TOKEN='your-jwt-token-here'"
    ],
    "create_product": "curl -X POST \"$API_URL/product/create\" -H \"Authorization: Bearer $JWT_TOKEN\" -H \"Content-Type: application/json\" -d '{\"qr_code\":\"TEST-001\",\"signal_type\":\"Pericolo - Test\",\"production_year\":2024,\"shape\":\"Triangolare\",\"dimension\":\"90x90cm\",\"wl_code\":\"WL-TEST\",\"support_material\":\"Alluminio\",\"support_thickness\":\"2.5mm\",\"fixation_class\":\"Classe RA2\",\"fixation_method\":\"Palo sostegno\",\"created_by\":\"test-user-id\"}'",
    "get_products": "curl -X GET \"$API_URL/product\" -H \"Authorization: Bearer $JWT_TOKEN\"",
    "create_maintenance": "curl -X POST \"$API_URL/maintenance/create\" -H \"Authorization: Bearer $JWT_TOKEN\" -H \"Content-Type: application/json\" -d '{\"intervention_type\":\"installazione\",\"gps_lat\":45.464211,\"gps_lng\":9.189982,\"year\":2024,\"poles_number\":2,\"company_id\":\"test-company\",\"certificate_number\":\"CERT-001\",\"reason\":\"Test\",\"notes\":\"Test maintenance\",\"product_uuid\":\"PRODUCT_UUID_HERE\"}'",
    "get_maintenances": "curl -X GET \"$API_URL/maintenance\" -H \"Authorization: Bearer $JWT_TOKEN\""
  }
